{
    "name": "blog-deployment",
    "version": "0.1.0",
    "bin": {
        "blog": "bin/blog.js"
    },
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc --watch",
        "cdk": "cdk",
        "format": "npm run lint -- --fix && prettier --loglevel error --write \"**/*.+(ts|json)\"",
        "lint": "eslint --ext .ts .",
        "prepare": "husky install"
    },
    "dependencies": {
        "@aws-cdk/aws-certificatemanager": "1.117.0",
        "@aws-cdk/aws-cloudfront": "^1.117.0",
        "@aws-cdk/aws-cloudfront-origins": "^1.117.0",
        "@aws-cdk/aws-cloudwatch": "^1.117.0",
        "@aws-cdk/aws-ec2": "1.117.0",
        "@aws-cdk/aws-elasticloadbalancingv2": "1.117.0",
        "@aws-cdk/aws-elasticloadbalancingv2-targets": "1.117.0",
        "@aws-cdk/aws-iam": "1.117.0",
        "@aws-cdk/aws-rds": "1.117.0",
        "@aws-cdk/aws-route53": "1.117.0",
        "@aws-cdk/aws-route53-targets": "1.117.0",
        "@aws-cdk/core": "1.117.0",
        "@types/joi": "^17.2.3",
        "@types/node": "^16.4.13",
        "aws-cdk": "1.117.0",
        "cdk-ec2-key-pair": "2.2.1",
        "dotenv": "^10.0.0",
        "joi": "^17.4.2",
        "source-map-support": "^0.5.19",
        "typescript": "^4.3.5"
    },
    "devDependencies": {
        "eslint": "^8.12.0",
        "eslint-config-prettier": "^8.5.0",
        "husky": "^7.0.4",
        "lint-staged": "^12.3.7",
        "prettier": "^2.6.1"
    }
}
